<h1>Listing json_samples</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @json_samples.each do |json_sample| %>
  <tr>
    <td><%= json_sample.title %></td>
    <td><%= json_sample.content %></td>
    <td><%= link_to 'Show', json_sample %></td>
    <td><%= link_to 'Edit', edit_json_sample_path(json_sample) %></td>
    <td><%= link_to 'Destroy', json_sample, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

